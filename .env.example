# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Evolution API Configuration
AUTHENTICATION_API_KEY="your-secret-api-key-here"

# Configurações de Rede (pode resolver problema de conectividade)
SERVER_TYPE=http
SERVER_PORT=8080
CORS_ORIGIN=*

# QR Code forçado  
QRCODE_GENERATE=true
WEBSOCKET_ENABLED=true

# Database mínima
DATABASE_ENABLED=true
DATABASE_PROVIDER=postgresql
DATABASE_CONNECTION_URI=postgresql://postgres:123456@postgres:5432/app_db?schema=public

# Redis mínima
CACHE_REDIS_ENABLED=true
CACHE_REDIS_URI=redis://redis:6379/6

# Postgres (para n8n)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=123456
POSTGRES_DB=app_db

# n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=root
N8N_BASIC_AUTH_PASSWORD=123456
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_USER=${POSTGRES_USER}
DB_POSTGRESDB_PASSWORD=${POSTGRES_PASSWORD}
DB_POSTGRESDB_DATABASE=${POSTGRES_DB}

CONFIG_SESSION_PHONE_VERSION=2.3000.1023204200

